git_COMMAND

Reference:http://youngxhui.top/2016/08/13/Git%E5%9F%BA%E6%9C%AC%E6%93%8D%E4%BD%9C/

 git clone 

 git pull

 git add -A

 git commit -m "abc"

 git push

 git p merge

// branch

 git checkout -b dev         // setup a new branch

 git branch                  //look up the current branch

 git add readme.txt

 git commit -m “branch test”

 git push

 git checkout master         // return to master branch

 git merge franklin          //merge branch

 git branch -d franklin      // delete branch


// Initial settings
git config –global user.name “Your Name”
$ git config –global user.email “email@example.com”

//clone from website
git clone link

// sychronize(first time)
git push --set-upstream origin test1
git push -u origin master

// sychronize(after)
git push origin master



cd到该文件夹
//删除文件夹下的所有 .svn 文件
find . -name ".svn" | xargs rm -Rf
//删除文件夹下的所有 .git 文件
find . -name ".git" | xargs rm -Rf


